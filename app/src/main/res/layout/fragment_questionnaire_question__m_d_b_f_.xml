<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".QuestionnaireWelcome">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <SeekBar
                android:id="@+id/seekBar9"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:indeterminate="false"
                android:max="100"
                android:progress="0"
                android:progressTint="@color/purple_500"
                android:splitTrack="false"
                android:thumbTint="@color/purple_500"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seekBar8" />

            <SeekBar
                android:id="@+id/seekBar10"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:indeterminate="false"
                android:max="100"
                android:progress="0"
                android:progressTint="@color/purple_500"
                android:splitTrack="false"
                android:thumbTint="@color/purple_500"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seekBar9" />

            <SeekBar
                android:id="@+id/seekBar11"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:indeterminate="false"
                android:max="100"
                android:progress="0"
                android:progressTint="@color/purple_500"
                android:splitTrack="false"
                android:thumbTint="@color/purple_500"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seekBar10" />

            <SeekBar
                android:id="@+id/seekBar12"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:indeterminate="false"
                android:max="100"
                android:progress="0"
                android:progressTint="@color/purple_500"
                android:splitTrack="false"
                android:thumbTint="@color/purple_500"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seekBar11" />

            <SeekBar
                android:id="@+id/seekBar8"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:indeterminate="false"
                android:max="100"
                android:progress="0"
                android:progressTint="@color/purple_500"
                android:splitTrack="false"
                android:thumbTint="@color/purple_500"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seekBar7" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:text="@string/question_MDBF"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:text="@string/tv_dissatisfied"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar7"
                app:layout_constraintEnd_toStartOf="@+id/seekBar7"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/seekBar7" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:text="@string/tv_restless_q1"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar8"
                app:layout_constraintEnd_toEndOf="@+id/textView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/seekBar8" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="7dp"
                android:text="@string/tv_uncomfortable"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar9"
                app:layout_constraintEnd_toStartOf="@+id/seekBar9"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/seekBar9" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:text="@string/tv_tense"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar10"
                app:layout_constraintEnd_toStartOf="@+id/seekBar10"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/seekBar10" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:text="@string/tv_lacking_energy"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar11"
                app:layout_constraintEnd_toStartOf="@+id/seekBar11"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/seekBar11" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:text="@string/tv_tired"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar12"
                app:layout_constraintEnd_toStartOf="@+id/seekBar12"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/seekBar12" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="@string/tv_satisfied_q1"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar7"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/seekBar7"
                app:layout_constraintTop_toTopOf="@+id/seekBar7" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="@string/tv_calm"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar8"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/textView9"
                app:layout_constraintTop_toTopOf="@+id/seekBar8" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="@string/tv_comfortable"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar9"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/seekBar9"
                app:layout_constraintTop_toTopOf="@+id/seekBar9" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="@string/tv_relaxed"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar10"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/seekBar10"
                app:layout_constraintTop_toTopOf="@+id/seekBar10" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="@string/tv_energetic"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar11"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/seekBar11"
                app:layout_constraintTop_toTopOf="@+id/seekBar11" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:layout_marginEnd="10dp"
                android:text="@string/tv_awake"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="@+id/seekBar12"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/seekBar12"
                app:layout_constraintTop_toTopOf="@+id/seekBar12" />

            <SeekBar
                android:id="@+id/seekBar7"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="54dp"
                android:indeterminate="false"
                android:max="100"
                android:progress="0"
                android:progressTint="@color/purple_500"
                android:splitTrack="false"
                android:thumbTint="@color/purple_500"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <Button
                android:id="@+id/button15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="52dp"
                android:onClick="onBtnBackClick_question_MDBF_Fragment"
                android:text="@string/btn_back"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/button16"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent" />

            <Button
                android:id="@+id/button16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="onBtnNextClick_question_MDBF_Fragment"
                android:text="@string/btn_next"
                app:layout_constraintBottom_toBottomOf="@+id/button15"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/button15" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </FrameLayout>

</ScrollView>
